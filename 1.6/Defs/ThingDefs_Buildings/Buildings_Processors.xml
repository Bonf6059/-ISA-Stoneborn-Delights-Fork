<ThingDef ParentName="BuildingBase">
  <defName>ISA_ShroomAleBarrel</defName>
  <label>dwarven cask</label>
  <description>A rustic sealed keg used to brew various stoneborn alcohols.</description>
  <drawerType>MapMeshAndRealTime</drawerType>
  <graphicData>
    <texPath>Things/Buildings/AleBarrel/AleCask</texPath>
    <drawSize>2</drawSize>
    <graphicClass>Graphic_Multi</graphicClass>
    <damageData>
      <rect>(0.05,0.1,0.9,0.9)</rect>
    </damageData>
    <shadowData>
      <volume>(0.75, 0.35, 0.33)</volume>
    </shadowData>
  </graphicData>
<size>(2,2)</size>
<costList>
  <WoodLog>120</WoodLog>
  <Steel>30</Steel>
</costList>
  <minifiedDef>MinifiedThing</minifiedDef>
  <defaultPlacingRot>North</defaultPlacingRot>
  <altitudeLayer>Building</altitudeLayer>
  <passability>PassThroughOnly</passability>
  <fillPercent>0.45</fillPercent>
  <pathCost>70</pathCost>
  <statBases>
    <WorkToBuild>1500</WorkToBuild>
    <Mass>28</Mass>
    <MaxHitPoints>120</MaxHitPoints>
    <Flammability>1</Flammability>
  </statBases>
  <comps>
<li Class="CompProperties_Forbiddable"/>
<li Class="ProcessorFramework.CompProperties_Processor">
  <capacity>30</capacity>
  <independentProcesses>false</independentProcesses>
  <parallelProcesses>false</parallelProcesses>
  <dropIngredients>false</dropIngredients>
  <showProductIcon>false</showProductIcon>
  <barOffset>(0.0, 0.45)</barOffset>
  <barScale>(0.3, 6)</barScale>
  <colorCoded>true</colorCoded>
  <processes>
    <li>ISA_ShroomaleBrewing</li>
    <li>ISA_SweetwineBrewing</li>
    <li>ISA_ElixirBrewing</li>
    <li>ISA_LiquorBrewing</li>
    <li>ISA_MeadBrewing</li>
  </processes>
</li>
</comps>
<tickerType>Rare</tickerType>
<rotatable>true</rotatable>
<designationCategory>Production</designationCategory>
  <uiOrder>2790</uiOrder>
  <building>
    <buildingTags>
      <li>Production</li>
    </buildingTags>
    <destroySound>BuildingDestroyed_Wood_Small</destroySound>
  </building>
  <researchPrerequisites>
   <li>ISA_DwarvenBrewing</li>
  </researchPrerequisites>
  <thingCategories>
    <li>BuildingsProduction</li>
  </thingCategories>
</ThingDef>

</Defs>
